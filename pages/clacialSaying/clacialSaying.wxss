  font-size: 30rpx;
}
.title {
  font-size: 36rpx;
  text-align: center;
  margin-bottom: 20rpx;
  margin-top: 30rpx;
  font-weight: bold;
}
.content {
  line-height: 1.5;
  margin-bottom: 10rpx;
  padding: 10rpx 10rpx;
  background-color: #f9f9f9;
}
