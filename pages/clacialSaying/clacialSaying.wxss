  margin-bottom: 50rpx;
}
.key-word view {
  color: #aaaaaa;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
}
.key-word input {
  padding: 30rpx 20rpx;
  background-color: #f8f8f8;
}
.placeholder-class {
  font-size: 32rpx;
}

.result {
  margin: 60rpx 30rpx;
  /* background-color: #f9f9f9; */
